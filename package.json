{
  "name": "kogefarm-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@binance-chain/bsc-connector": "^1.0.0",
    "@ethersproject/abi": "^5.1.2",
    "@pancakeswap/uikit": "^0.32.2",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "chart.js": "2.9.4",
    "date-fns": "^2.21.3",
    "ethers": "^5.1.4",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "quickswap-sdk": "^3.0.8",
    "react": "^17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-countdown": "^2.3.2",
    "react-countup": "^4.3.3",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-helmet-async": "^1.0.9",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-scroll-to-top": "^1.0.5",
    "react-tippy": "^1.4.0",
    "react-transition-group": "^4.4.1",
    "split-grid": "^1.0.11",
    "styled-components": "^5.3.0",
    "swiper": "^6.6.1",
    "tippy.js": "^6.3.1",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "urql": "^2.0.3",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@pancakeswap/eslint-config-pancake": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/react-redux": "^7.1.16",
    "@types/react-transition-group": "^4.4.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.26.0",
    "html-webpack-plugin": "4",
    "interpolate-html-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.3.0",
    "resolve-typescript-plugin": "^1.1.1",
    "source-map-explorer": "^2.5.2",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "ts-unused-exports": "^7.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
